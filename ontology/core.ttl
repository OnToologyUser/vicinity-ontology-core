@prefix : <http://iot.linkeddata.es/def/core#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix vs: <http://www.w3.org/2003/06/sw-vocab-status/ns#> .
@prefix geo: <http://www.w3.org/2003/01/geo/wgs84_pos#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix ssn: <http://purl.oclc.org/NET/ssnx/ssn#> .
@prefix wot: <http://xmlns.com/wot/0.1/> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix saref: <https://w3id.org/saref#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@base <http://iot.linkeddata.es/def/core#> .

<http://iot.linkeddata.es/def/core#> rdf:type owl:Ontology ;
                                      owl:imports <http://iot.linkeddata.es/def/wot#> ,
                                                  <http://www.w3.org/ns/org#> ;
                                      dcterms:creator <http://purl.org/net/mpoveda> ;
                                      rdfs:comment "This ontology represent the core terms to allow interoperability in an IoT context based on  VICINITY technological solutions."@en ;
                                      dcterms:creator <http://www.garcia-castro.com/foaf.rdf#me> ;
                                      dcterms:contributor <http://www.oeg-upm.net/index.php/en/universitystaff/402-fserena/> ;
                                      owl:versionInfo "0.0.5" ;
                                      dcterms:license <http://purl.org/NET/rdflicense/cc-by4.0> ;
                                      vann:preferredNamespacePrefix "vcnt-core" ;
                                      dc:publisher <http://www.oeg-upm.net/> ;
                                      vann:preferredNamespaceUri "http://iot.linkeddata.es/def/core" ;
                                      dc:title "Vicinity core model"@en .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/elements/1.1/source
dc:source rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/contributor
dcterms:contributor rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2003/06/sw-vocab-status/ns#term_status
vs:term_status rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://iot.linkeddata.es/def/core#actsOn
:actsOn rdf:type owl:ObjectProperty ;
        rdfs:comment "A link between an actuator an the entities or properties upon which it can act." ;
        rdfs:label "acts on"@en ;
        rdfs:seeAlso "ISO/IEC CC 30141. Information technology - Internet of Things Reference architecutre (IoT RA)" .


###  http://iot.linkeddata.es/def/core#avatar
:avatar rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf foaf:depiction ;
        rdfs:comment "A link from an entity to the image identifying it in a social network or system."@en ;
        rdfs:label "avatar"@en .


###  http://iot.linkeddata.es/def/core#contains
:contains rdf:type owl:ObjectProperty ;
          rdfs:comment "A property between a thing and its components. For example, virtual things can contain virtual things while physical things can contain physicial things."@en ;
          rdfs:label "contains"@en ;
          rdfs:seeAlso "ISO/IEC CC 30141. Information technology - Internet of Things Reference architecutre (ioT RA)" .


###  http://iot.linkeddata.es/def/core#describes
:describes rdf:type owl:ObjectProperty ;
           owl:inverseOf :isDescribedBy ;
           rdfs:comment "Link from a thing description to the thing being described"@en ;
           rdfs:label "describes thing"@en .


###  http://iot.linkeddata.es/def/core#describesEcosystem
:describesEcosystem rdf:type owl:ObjectProperty ;
                    owl:inverseOf :isEcosystemDescribedBy ;
                    rdf:type owl:FunctionalProperty ;
                    rdfs:comment "Link between a thing ecosystem description and the ecosystem being described."@en ;
                    rdfs:label "describes ecosystem"@en ;
                    owl:deprecated "true"^^xsd:boolean .


###  http://iot.linkeddata.es/def/core#expressedInFormat
:expressedInFormat rdf:type owl:ObjectProperty ;
                   rdfs:domain :Value ;
                   rdfs:range <http://iot.linkeddata.es/def/wot#DataSchema> ;
                   rdfs:comment "Link from a value to the data format in which it is expressed."@en ;
                   rdfs:label "expressed in format"@en .


###  http://iot.linkeddata.es/def/core#hasComponent
:hasComponent rdf:type owl:ObjectProperty ;
              owl:inverseOf :isComponentOf ;
              rdfs:comment "A link between an ecosystem and the things that compose such ecosystem."@en ;
              rdfs:label "has component"@en .


###  http://iot.linkeddata.es/def/core#hasNeighbourhood
:hasNeighbourhood rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf :isInvolvedIn ;
                  owl:inverseOf :isNeighbourhoodOf ;
                  rdf:type owl:FunctionalProperty ;
                  rdfs:domain :Agent ;
                  rdfs:range :Neighbourhood ;
                  rdfs:isDefinedBy "The link between an agent and its neighbourhood. Such neighbourhood will link to all the entities involved in such neighbourhood."@en ;
                  rdfs:label "has neighbourhood"@en .


###  http://iot.linkeddata.es/def/core#hasOwner
:hasOwner rdf:type owl:ObjectProperty ;
          owl:inverseOf :owns ;
          rdfs:isDefinedBy "Link between an entity like a service or a device and its owners or providers."@en ;
          rdfs:label "has owner"@en ,
                     "has provider"@en .


###  http://iot.linkeddata.es/def/core#hasPartnershipWith
:hasPartnershipWith rdf:type owl:ObjectProperty ,
                             owl:SymmetricProperty ;
                    rdfs:domain <http://www.w3.org/ns/org#Organization> ;
                    rdfs:range <http://www.w3.org/ns/org#Organization> ;
                    rdfs:isDefinedBy "A link between two organizations who are in a partnership relation. A partnership is established between organizations, only 2 organizations."@en ;
                    rdfs:label "hasPartnershipWith"@en .


###  http://iot.linkeddata.es/def/core#hasValue
:hasValue rdf:type owl:ObjectProperty ;
          rdfs:domain <http://iot.linkeddata.es/def/wot#InteractionPattern> ;
          rdfs:range :Value ;
          rdfs:comment "Indicates a value provided by an interaction pattern. In case one ore more values are indicated the order will be defined by the time stamp attributes."@en ;
          rdfs:label "has value"@en .


###  http://iot.linkeddata.es/def/core#hasVendor
:hasVendor rdf:type owl:ObjectProperty ;
           rdfs:isDefinedBy "Link from a device to its vendor or manufacturer or producer."@en ;
           rdfs:label "has producer"@en ,
                      "has vendor"@en .


###  http://iot.linkeddata.es/def/core#interactsUsing
:interactsUsing rdf:type owl:ObjectProperty ;
                rdfs:isDefinedBy "Linkg between an agent and the virtual thing it interatcs with."@en ;
                rdfs:label "interacts using"@en ;
                rdfs:seeAlso "ISO/IEC CC 30141. Information technology - Internet of Things Reference architecutre (IoT RA)" .


###  http://iot.linkeddata.es/def/core#involves
:involves rdf:type owl:ObjectProperty ;
          owl:inverseOf :isInvolvedIn ;
          rdfs:isDefinedBy "A link between a neighbourhood and all the agents involved in it."@en ;
          rdfs:label "involves"@en .


###  http://iot.linkeddata.es/def/core#isComponentOf
:isComponentOf rdf:type owl:ObjectProperty ;
               rdfs:comment "A link between a thing and the ecosystems it belongs to."@en ;
               rdfs:label "is component of"@en .


###  http://iot.linkeddata.es/def/core#isDescribedBy
:isDescribedBy rdf:type owl:ObjectProperty ;
               rdfs:comment "Relation from a thing to its thing descriptions"@en ;
               rdfs:label "is described by"@en .


###  http://iot.linkeddata.es/def/core#isEcosystemDescribedBy
:isEcosystemDescribedBy rdf:type owl:ObjectProperty ,
                                 owl:InverseFunctionalProperty ;
                        rdfs:comment "Link from an ecosystem to the thing ecosystem descriptions that provide information about such ecosystem."@en ;
                        rdfs:label "is ecosystem described by"@en ;
                        owl:deprecated "true"^^xsd:boolean .


###  http://iot.linkeddata.es/def/core#isInvolvedIn
:isInvolvedIn rdf:type owl:ObjectProperty ;
              rdfs:isDefinedBy "A link between an agent and all the neighbouthoods it is involved in."@en ;
              rdfs:label "is involved in"@en .


###  http://iot.linkeddata.es/def/core#isNeighbourhoodOf
:isNeighbourhoodOf rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf :involves ;
                   rdf:type owl:FunctionalProperty ;
                   rdfs:domain :Neighbourhood ;
                   rdfs:range :Agent ;
                   rdfs:isDefinedBy "A link between a neighbourhood and the specific agent around which the neighbourhood is built."@en ;
                   rdfs:label "is neighbourhood of"@en .


###  http://iot.linkeddata.es/def/core#isRelativeTo
:isRelativeTo rdf:type owl:ObjectProperty ;
              rdfs:comment "Link from a relative endpoint to the (non relative) endpoint to which it is relative."@en ;
              rdfs:label "is relative to"@en .


###  http://iot.linkeddata.es/def/core#literalValue
:literalValue rdf:type owl:ObjectProperty ;
              rdfs:domain :Value ;
              rdfs:range rdfs:Resource ;
              rdfs:comment "Literal representation of the observed value."@en ;
              rdfs:label "literal value"@en .


###  http://iot.linkeddata.es/def/core#monitors
:monitors rdf:type owl:ObjectProperty ;
          rdfs:isDefinedBy "A link between a sensor an the entities or properties it can monitor."@en ;
          rdfs:label "monitors"@en ;
          rdfs:seeAlso "ISO/IEC CC 30141. Information technology - Internet of Things Reference architecutre (IoT RA)" .


###  http://iot.linkeddata.es/def/core#owns
:owns rdf:type owl:ObjectProperty ;
      rdfs:isDefinedBy "A linkd between an agent and the entities, like devices or services, it provides or owns."@en ;
      rdfs:label "owns"@en ,
                 "provides"@en .


###  http://purl.oclc.org/NET/ssnx/ssn#forProperty
ssn:forProperty rdf:type owl:ObjectProperty .


###  http://purl.oclc.org/NET/ssnx/ssn#hasMeasurementProperty
ssn:hasMeasurementProperty rdf:type owl:ObjectProperty .


###  http://purl.oclc.org/NET/ssnx/ssn#hasOperatingProperty
ssn:hasOperatingProperty rdf:type owl:ObjectProperty .


###  http://purl.oclc.org/NET/ssnx/ssn#hasSurvivalProperty
ssn:hasSurvivalProperty rdf:type owl:ObjectProperty .


###  http://purl.oclc.org/NET/ssnx/ssn#inCondition
ssn:inCondition rdf:type owl:ObjectProperty .


###  http://purl.oclc.org/NET/ssnx/ssn#isPropertyOf
ssn:isPropertyOf rdf:type owl:ObjectProperty .


###  http://www.w3.org/2003/01/geo/wgs84_pos#location
geo:location rdf:type owl:ObjectProperty .


###  http://xmlns.com/foaf/0.1/depiction
foaf:depiction rdf:type owl:ObjectProperty .


###  https://w3id.org/def/saref4bldg#isContainedIn
<https://w3id.org/def/saref4bldg#isContainedIn> rdf:type owl:ObjectProperty ;
                                                rdfs:comment "A relation between an object and the physical space in which it is located."@en ;
                                                rdfs:isDefinedBy "https://w3id.org/def/saref4bldg#"@en ;
                                                rdfs:label "is contained in"@en .


#################################################################
#    Data properties
#################################################################

###  http://iot.linkeddata.es/def/core#desctiption
:desctiption rdf:type owl:DatatypeProperty ;
             rdfs:range xsd:string ;
             rdfs:comment "General attribute for descriptions assigned to any type of concept" ;
             rdfs:label "desctiption"@en .


###  http://iot.linkeddata.es/def/core#deviceDescription
:deviceDescription rdf:type owl:DatatypeProperty ;
                   rdfs:subPropertyOf :desctiption ;
                   rdfs:domain :Device ;
                   rdfs:range xsd:string ;
                   rdfs:comment "Natural language description of a device."@en ;
                   rdfs:label "device description"@en .


###  http://iot.linkeddata.es/def/core#deviceName
:deviceName rdf:type owl:DatatypeProperty ;
            rdfs:subPropertyOf <http://iot.linkeddata.es/def/wot#name> ;
            rdfs:domain :Device ;
            rdfs:range xsd:string ;
            rdfs:comment "Name of a device" ;
            rdfs:label "device name"@en .


###  http://iot.linkeddata.es/def/core#relativeHref
:relativeHref rdf:type owl:DatatypeProperty ;
              rdfs:subPropertyOf <http://iot.linkeddata.es/def/wot#href> ;
              rdfs:domain :RelativeEndpoint ;
              rdfs:range xsd:string ;
              rdfs:label "relative href"@en .


###  http://iot.linkeddata.es/def/core#serialNumber
:serialNumber rdf:type owl:DatatypeProperty ;
              rdfs:domain :Device ;
              rdfs:range xsd:string ;
              rdfs:comment "Device serial number" ;
              rdfs:label "serialNumber"@en .


###  http://iot.linkeddata.es/def/core#serviceDescription
:serviceDescription rdf:type owl:DatatypeProperty ;
                    rdfs:subPropertyOf :desctiption ;
                    rdfs:domain :Service ;
                    rdfs:range xsd:string ;
                    rdfs:comment "Natural language description of a service."@en ;
                    rdfs:label "service description"@en .


###  http://iot.linkeddata.es/def/core#serviceName
:serviceName rdf:type owl:DatatypeProperty ;
             rdfs:subPropertyOf <http://iot.linkeddata.es/def/wot#name> ;
             rdfs:domain :Service ;
             rdfs:range xsd:string ;
             rdfs:comment "Name of a service" ;
             rdfs:label "service name"@en .


###  http://iot.linkeddata.es/def/core#timeStamp
:timeStamp rdf:type owl:DatatypeProperty ,
                    owl:FunctionalProperty ;
           rdfs:range xsd:dateTime ;
           rdfs:comment "Time stamp in which a phenomenon takes place, for example when value has been taken."@en ;
           rdfs:label "time stamp"@en .


#################################################################
#    Classes
#################################################################

###  http://iot.linkeddata.es/def/core#Actuator
:Actuator rdf:type owl:Class ;
          rdfs:subClassOf :Device ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :actsOn ;
                            owl:allValuesFrom [ rdf:type owl:Class ;
                                                owl:unionOf ( <http://iot.linkeddata.es/def/wot#PhysicalThing>
                                                              ssn:Property
                                                            )
                                              ]
                          ] ;
          rdfs:comment "An actuator is a device that accepts digital inputs and which acts on (changes) one or more properties of a physical entity on the basis of those inputs. An actuator is a specialization of an IoT device. An actuator acts on a physical entity. (definition taken from ISO/IEC 11404:2007)"@en ;
          rdfs:label "Actuator"@en ;
          rdfs:seeAlso "ISO/IEC CC 30141. Information technology - Internet of Things Reference architecutre (IoT RA)"@en .


###  http://iot.linkeddata.es/def/core#Agent
:Agent rdf:type owl:Class ;
       rdfs:subClassOf foaf:Agent ;
       rdfs:comment "An agent is a person or piece of software involved in a VICINITY neighbourhood."@en ;
       rdfs:label "Agent"@en .


###  http://iot.linkeddata.es/def/core#AmbientProperty
:AmbientProperty rdf:type owl:Class ;
                 rdfs:subClassOf ssn:Property ;
                 rdfs:label "AmbientProperty"@en .


###  http://iot.linkeddata.es/def/core#Application
:Application rdf:type owl:Class ;
             rdfs:subClassOf :Service ;
             rdfs:comment "An application is a software entity or system that offers a collection of functions with which a user can perform a task or achieve a business objective. An application typically uses services. The functions of an application may be presented as services. (definition taken from ISO/IEC 11404:2007)"@en ;
             rdfs:label "Application"@en ;
             rdfs:seeAlso "ISO/IEC CC 30141. Information technology - Internet of Things Reference architecutre (IoT RA)"@en .


###  http://iot.linkeddata.es/def/core#CO2Sensor
:CO2Sensor rdf:type owl:Class ;
           rdfs:subClassOf :Sensor ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :monitors ;
                             owl:someValuesFrom :CarbonDioxide
                           ] ;
           rdfs:label "CO2Sensor"@en .


###  http://iot.linkeddata.es/def/core#CarbonDioxide
:CarbonDioxide rdf:type owl:Class ;
               rdfs:subClassOf ssn:Property ;
               rdfs:label "CarbonDioxide"@en .


###  http://iot.linkeddata.es/def/core#CarbonMonoxide
:CarbonMonoxide rdf:type owl:Class ;
                rdfs:subClassOf ssn:Property ;
                rdfs:label "CarbonMonoxide"@en .


###  http://iot.linkeddata.es/def/core#Charger
:Charger rdf:type owl:Class ;
         rdfs:subClassOf :Device ;
         rdfs:label "Charger"@en .


###  http://iot.linkeddata.es/def/core#Device
:Device rdf:type owl:Class ;
        rdfs:subClassOf :PhysicalThing ;
        rdfs:comment "An IoT device is a digital entity which bridges between real-world physical entities and the other digital entities of an IoT system. IoT device interacts one or more networks through which interactions are made with other entities. IoT device exposes one or more endpoints by which interactions are made. (definition taken from ISO/IEC 11404:2007)"@en ;
        rdfs:label "Device"@en ;
        rdfs:seeAlso "ISO/IEC CC 30141. Information technology - Internet of Things Reference architecutre (IoT RA)"@en .


###  http://iot.linkeddata.es/def/core#DigitalUser
:DigitalUser rdf:type owl:Class ;
             rdfs:subClassOf foaf:Agent ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :interactsUsing ;
                               owl:allValuesFrom :Service
                             ] ;
             rdfs:comment "A digital user is a digital entity which uses an IoT system. A digital user is a specialization of an IoT-User. A digital user interacts with one or more services offered by the IoT system across the network. (definition taken from ISO/IEC 11404:2007)"@en ;
             rdfs:label "Digital user"@en ;
             rdfs:seeAlso "ISO/IEC CC 30141. Information technology - Internet of Things Reference architecutre (IoT RA)"@en .


###  http://iot.linkeddata.es/def/core#EBikeCharger
:EBikeCharger rdf:type owl:Class ;
              rdfs:subClassOf :Charger ;
              rdfs:label "eBikeCharger"@en .


###  http://iot.linkeddata.es/def/core#Ecosystem
:Ecosystem rdf:type owl:Class ;
           rdfs:subClassOf [ rdf:type owl:Restriction ;
                             owl:onProperty :isDescribedBy ;
                             owl:someValuesFrom :ThingEcosystemDescription
                           ] ;
           rdfs:comment "Collection of things that co-exist in a given environment."@en ;
           rdfs:label "Ecosystem"@en .


###  http://iot.linkeddata.es/def/core#HVACSensor
:HVACSensor rdf:type owl:Class ;
            rdfs:subClassOf :Actuator ,
                            :Sensor ;
            rdfs:label "HVACSensor"@en .


###  http://iot.linkeddata.es/def/core#HumanUser
:HumanUser rdf:type owl:Class ;
           rdfs:subClassOf foaf:Person ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :interactsUsing ;
                             owl:allValuesFrom :Application
                           ] ;
           rdfs:comment "A human user is person who uses an IoT system. A human user is a specialization of an IoT-User. A human user interacts across the network via an application. (definition taken from ISO/IEC 11404:2007)"@en ;
           rdfs:label "Human user"@en ;
           rdfs:seeAlso "ISO/IEC CC 30141. Information technology - Internet of Things Reference architecutre (IoT RA)"@en .


###  http://iot.linkeddata.es/def/core#HumidityAmbientProperty
:HumidityAmbientProperty rdf:type owl:Class ;
                         rdfs:subClassOf :AmbientProperty ;
                         rdfs:label "HumidityAmbientProperty"@en .


###  http://iot.linkeddata.es/def/core#HumiditySensor
:HumiditySensor rdf:type owl:Class ;
                rdfs:subClassOf :Sensor ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty :monitors ;
                                  owl:someValuesFrom :HumidityAmbientProperty
                                ] ;
                rdfs:label "HumiditySensor"@en .


###  http://iot.linkeddata.es/def/core#Irradiance
:Irradiance rdf:type owl:Class ;
            rdfs:subClassOf :LightProperty ;
            rdfs:label "Irradiance"@en .


###  http://iot.linkeddata.es/def/core#LightProperty
:LightProperty rdf:type owl:Class ;
               rdfs:subClassOf ssn:Property ;
               rdfs:label "LightProperty"@en .


###  http://iot.linkeddata.es/def/core#LightSwitch
:LightSwitch rdf:type owl:Class ;
             rdfs:subClassOf :Actuator ;
             rdfs:label "LightSwitch"@en .


###  http://iot.linkeddata.es/def/core#Lightbulb
:Lightbulb rdf:type owl:Class ;
           rdfs:subClassOf :Device ;
           rdfs:label "Lightbulb"@en .


###  http://iot.linkeddata.es/def/core#LuminanceMeter
:LuminanceMeter rdf:type owl:Class ;
                rdfs:subClassOf :Sensor ;
                rdfs:label "LuminanceMeter"@en .


###  http://iot.linkeddata.es/def/core#LuminusFlux
:LuminusFlux rdf:type owl:Class ;
             rdfs:subClassOf :LightProperty ;
             rdfs:label "Luminus flux"@en .


###  http://iot.linkeddata.es/def/core#LuminusIntensity
:LuminusIntensity rdf:type owl:Class ;
                  rdfs:subClassOf :LightProperty ;
                  rdfs:label "LuminusIntensity"@en .


###  http://iot.linkeddata.es/def/core#Motion
:Motion rdf:type owl:Class ;
        rdfs:subClassOf ssn:Property ;
        rdfs:label "Motion"@en .


###  http://iot.linkeddata.es/def/core#MotionSensor
:MotionSensor rdf:type owl:Class ;
              rdfs:subClassOf :Sensor ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :monitors ;
                                owl:someValuesFrom :Motion
                              ] ;
              rdfs:label "MotionSensor"@en .


###  http://iot.linkeddata.es/def/core#Neighbourhood
:Neighbourhood rdf:type owl:Class ;
               rdfs:comment "A set of IoT infrastructures and value-added services integrated through a network of VICINITY Nodes, where the infrastructures can share the access to their IoT objects without losing the control over them."@en ;
               rdfs:label "Neighbourhood"@en .


###  http://iot.linkeddata.es/def/core#NoiseSensor
:NoiseSensor rdf:type owl:Class ;
             rdfs:subClassOf :Sensor ;
             rdfs:label "NoiseSensor"@en .


###  http://iot.linkeddata.es/def/core#OccupancySensor
:OccupancySensor rdf:type owl:Class ;
                 rdfs:subClassOf :Sensor ;
                 rdfs:label "OccupancySensor"@en .


###  http://iot.linkeddata.es/def/core#OperationalStatus
:OperationalStatus rdf:type owl:Class ;
                   rdfs:subClassOf ssn:Property ;
                   rdfs:label "Operational status"@en .


###  http://iot.linkeddata.es/def/core#PeopleCounter
:PeopleCounter rdf:type owl:Class ;
               rdfs:subClassOf :Sensor ;
               rdfs:comment "A people counter is a device that is used to measure the number of people traversing a certain passage or entrance. (Definition taken from https://en.wikipedia.org/wiki/People_counter)"@en ;
               rdfs:label "People counter"@en .


###  http://iot.linkeddata.es/def/core#PhysicalThing
:PhysicalThing rdf:type owl:Class ;
               rdfs:subClassOf <http://iot.linkeddata.es/def/wot#Thing> ,
                               geo:SpatialThing ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :contains ;
                                 owl:allValuesFrom :PhysicalThing
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty geo:location ;
                                 owl:allValuesFrom geo:SpatialThing
                               ] ;
               rdfs:comment """A physical entity is a real-world thing. A physical entity is a specialization of entity. A physical entity may contain other physical entities. A physical entity is a discrete, identifiable part of the physical environment that is of interest to the user for the completion of her goal. Physical entities can be almost any physical object or environment; from humans or animals to cars; from store or logistics chain items to computers; from electronic appliances to closed or open environments.

(Definition taken from 
ISO/IEC CC 30141. Information technology - Internet of Things Reference architecutre (IoT RA))"""@en ,
                            "Any entity that has a proper space region."@en ;
               rdfs:label "Physical thing"@en ;
               rdfs:seeAlso "ISO/IEC CC 30141. Information technology - Internet of Things Reference architecutre (IoT RA)"@en .


###  http://iot.linkeddata.es/def/core#Presence
:Presence rdf:type owl:Class ;
          rdfs:subClassOf ssn:Property ;
          rdfs:label "Presence"@en .


###  http://iot.linkeddata.es/def/core#Radiance
:Radiance rdf:type owl:Class ;
          rdfs:subClassOf :LightProperty ;
          rdfs:label "Radiance"@en .


###  http://iot.linkeddata.es/def/core#RadiantIntensity
:RadiantIntensity rdf:type owl:Class ;
                  rdfs:subClassOf :LightProperty ;
                  rdfs:label "Radiant intensity"@en .


###  http://iot.linkeddata.es/def/core#RelativeEndpoint
:RelativeEndpoint rdf:type owl:Class ;
                  rdfs:subClassOf <http://iot.linkeddata.es/def/wot#Link> ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty :isRelativeTo ;
                                    owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                    owl:onClass [ owl:intersectionOf ( <http://iot.linkeddata.es/def/wot#Link>
                                                                       [ rdf:type owl:Class ;
                                                                         owl:complementOf :RelativeEndpoint
                                                                       ]
                                                                     ) ;
                                                  rdf:type owl:Class
                                                ]
                                  ] ;
                  rdfs:comment "An endpoint which is relative (its href is composed by a relative path and an absolute one given by another endpoint) to an endpoint which can not be relative."@en ;
                  rdfs:label "Relative endpoint"@en .


###  http://iot.linkeddata.es/def/core#Sensor
:Sensor rdf:type owl:Class ;
        rdfs:subClassOf :Device ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :monitors ;
                          owl:allValuesFrom [ rdf:type owl:Class ;
                                              owl:unionOf ( <http://iot.linkeddata.es/def/wot#PhysicalThing>
                                                            ssn:Property
                                                          )
                                            ]
                        ] ;
        rdfs:comment "A sensor is a device that detects and responds to some type of input from the physical environment and outputs digital data that can be transmitted over a network. A sensor is a specialization of an IoT device. A sensor monitors a physical entity. The output digital data may only be distantly related to the input from the physical environment and may be output with a significant time delay, depending on processing of the data within the device, for example, the recognition of the identity of a person from a surveillance camera device. (definition taken from ISO/IEC 11404:2007)"@en ;
        rdfs:label "Sensor"@en ;
        rdfs:seeAlso "ISO/IEC CC 30141. Information technology - Internet of Things Reference architecutre (IoT RA)"@en .


###  http://iot.linkeddata.es/def/core#Service
:Service rdf:type owl:Class ;
         rdfs:subClassOf :VirtualThing ;
         rdfs:comment "A service is a set of distinct capabilities provided by a software component through a defined interface, which may be composed of other services. A service is implemented by one or more components. A service defines network interfaces and exposed by an Endpoint. (definition taken from ISO/IEC 11404:2007)"@en ;
         rdfs:label "Service"@en ;
         rdfs:seeAlso "ISO/IEC CC 30141. Information technology - Internet of Things Reference architecutre (IoT RA)"@en .


###  http://iot.linkeddata.es/def/core#TemperatureAmbientProperty
:TemperatureAmbientProperty rdf:type owl:Class ;
                            rdfs:subClassOf :AmbientProperty ;
                            rdfs:label "TemperatureAmbientProperty"@en .


###  http://iot.linkeddata.es/def/core#Thermometer
:Thermometer rdf:type owl:Class ;
             rdfs:subClassOf :Sensor ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :monitors ;
                               owl:someValuesFrom :TemperatureAmbientProperty
                             ] ;
             rdfs:label "Thermometer"@en .


###  http://iot.linkeddata.es/def/core#Thermostat
:Thermostat rdf:type owl:Class ;
            rdfs:subClassOf :Device ;
            rdfs:label "Thermostat"@en .


###  http://iot.linkeddata.es/def/core#ThingDescription
:ThingDescription rdf:type owl:Class ;
                  rdfs:subClassOf [ rdf:type owl:Restriction ;
                                    owl:onProperty :describes ;
                                    owl:someValuesFrom <http://iot.linkeddata.es/def/wot#Thing>
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty :describes ;
                                    owl:allValuesFrom <http://iot.linkeddata.es/def/wot#Thing>
                                  ] ;
                  rdfs:comment "A digital representation encapsulates a physical object accessible via Web services"@en ;
                  rdfs:label "Thing description"@en .


###  http://iot.linkeddata.es/def/core#ThingEcosystemDescription
:ThingEcosystemDescription rdf:type owl:Class ;
                           rdfs:subClassOf [ rdf:type owl:Restriction ;
                                             owl:onProperty :describes ;
                                             owl:someValuesFrom :Ecosystem
                                           ] ;
                           rdfs:comment "A digital representation that encapsulates an ecosystem taht is accessible via Web services" ;
                           rdfs:label "Thing ecosystem description"@en .


###  http://iot.linkeddata.es/def/core#Value
:Value rdf:type owl:Class ;
       rdfs:subClassOf [ rdf:type owl:Restriction ;
                         owl:onProperty :expressedInFormat ;
                         owl:allValuesFrom <http://iot.linkeddata.es/def/wot#DataSchema>
                       ] ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :literalValue ;
                         owl:cardinality "1"^^xsd:nonNegativeInteger
                       ] ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :timeStamp ;
                         owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                         owl:onDataRange xsd:dateTime
                       ] ;
       rdfs:comment "Class representing values provided by an interaction pattern in a given time stamp. It might also indicated the data format in which the value is expressed."@en ;
       rdfs:label "Value"@en .


###  http://iot.linkeddata.es/def/core#VirtualThing
:VirtualThing rdf:type owl:Class ;
              rdfs:subClassOf <http://iot.linkeddata.es/def/wot#Thing> ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :contains ;
                                owl:allValuesFrom :VirtualThing
                              ] ;
              rdfs:comment """A virtual entity is a digital representation of a physical entity, contained within a service. A virtual entity interacts through an endpoint. A virtual entity represents a physical entity.

(Definition taken from 
ISO/IEC CC 30141. Information technology - Internet of Things Reference architecutre (IoT RA))"""@en ;
              rdfs:label "Virtual thing"@en ;
              rdfs:seeAlso "ISO/IEC CC 30141. Information technology - Internet of Things Reference architecutre (ioT RA)"@en .


###  http://iot.linkeddata.es/def/wot#Action
<http://iot.linkeddata.es/def/wot#Action> rdfs:subClassOf [ rdf:type owl:Restriction ;
                                                            owl:onProperty :actsOn ;
                                                            owl:someValuesFrom <http://purl.oclc.org/NET/ssnx/ssn#Property>
                                                          ] .


###  http://iot.linkeddata.es/def/wot#PhysicalThing
<http://iot.linkeddata.es/def/wot#PhysicalThing> rdf:type owl:Class .


###  http://iot.linkeddata.es/def/wot#Property
<http://iot.linkeddata.es/def/wot#Property> rdfs:subClassOf [ rdf:type owl:Restriction ;
                                                              owl:onProperty :monitors ;
                                                              owl:someValuesFrom <http://purl.oclc.org/NET/ssnx/ssn#Property>
                                                            ] .


###  http://purl.oclc.org/NET/ssnx/ssn#Accuracy
ssn:Accuracy rdf:type owl:Class ;
             rdfs:subClassOf ssn:MeasurementProperty ;
             dc:source """skos:exactMatch 'measurement accuracy/accuracy' [VIM 2.13] 
		                    http://www.bipm.org/utils/common/documents/jcgm/JCGM_200_2008.pdf""" ;
             rdfs:comment "The closeness of agreement between the value of an observation and the true value of the observed quality." ;
             rdfs:isDefinedBy "http://purl.oclc.org/NET/ssnx/ssn" ;
             rdfs:label "Accuracy" ;
             rdfs:seeAlso "http://www.w3.org/2005/Incubator/ssn/wiki/SSN_Sensor#MeasuringCapability" .


###  http://purl.oclc.org/NET/ssnx/ssn#BatteryLifetime
ssn:BatteryLifetime rdf:type owl:Class ;
                    rdfs:subClassOf ssn:SurvivalProperty ;
                    rdfs:comment "Total useful life of a battery." ;
                    rdfs:isDefinedBy "http://purl.oclc.org/NET/ssnx/ssn" ;
                    rdfs:label "Battery Lifetime" ;
                    rdfs:seeAlso "http://www.w3.org/2005/Incubator/ssn/wiki/SSN_Energy#EnergyRestriction" .


###  http://purl.oclc.org/NET/ssnx/ssn#Condition
ssn:Condition rdf:type owl:Class ;
              rdfs:subClassOf ssn:Property ;
              rdfs:comment "Used to specify ranges for qualities that act as conditions on a system/sensor's operation.  For example, wind speed of 10-60m/s is expressed as a condition linking a quality, wind speed, a unit of measurement, metres per second, and a set of values, 10-60, and may be used as the condition on a MeasurementProperty, for example, to state that a sensor has a particular accuracy in that condition." ;
              rdfs:isDefinedBy "http://purl.oclc.org/NET/ssnx/ssn" ;
              rdfs:label "Condition" ;
              rdfs:seeAlso "http://www.w3.org/2005/Incubator/ssn/wiki/SSN_Base#ConstraintBlock" .


###  http://purl.oclc.org/NET/ssnx/ssn#DetectionLimit
ssn:DetectionLimit rdf:type owl:Class ;
                   rdfs:subClassOf ssn:MeasurementProperty ;
                   dc:source """skos:exactMatch 'detection limit' [VIM 4.18] 
                                 http://www.bipm.org/utils/common/documents/jcgm/JCGM_200_2008.pdf""" ;
                   rdfs:comment "An observed value for which the probability of falsely claiming the absence of a component in a material is Î², given a probability Î± of falsely claiming its presence." ;
                   rdfs:isDefinedBy "http://purl.oclc.org/NET/ssnx/ssn" ;
                   rdfs:label "detection limit" ;
                   rdfs:seeAlso "http://www.w3.org/2005/Incubator/ssn/wiki/SSN_Sensor#MeasuringCapability" .


###  http://purl.oclc.org/NET/ssnx/ssn#Drift
ssn:Drift rdf:type owl:Class ;
          rdfs:subClassOf ssn:MeasurementProperty ;
          dc:source """skos:exactMatch 'instrumental drift' [VIM 4.21]
                                  http://www.bipm.org/utils/common/documents/jcgm/JCGM_200_2008.pdf""" ;
          rdfs:comment "A, continuous or incremental, change in the reported values of observations over time for an unchanging quality." ;
          rdfs:isDefinedBy "http://purl.oclc.org/NET/ssnx/ssn" ;
          rdfs:label "Drift" ;
          rdfs:seeAlso "http://www.w3.org/2005/Incubator/ssn/wiki/SSN_Sensor#MeasuringCapability" .


###  http://purl.oclc.org/NET/ssnx/ssn#FeatureOfInterest
ssn:FeatureOfInterest rdf:type owl:Class .


###  http://purl.oclc.org/NET/ssnx/ssn#Frequency
ssn:Frequency rdf:type owl:Class ;
              rdfs:subClassOf ssn:MeasurementProperty ;
              rdfs:comment "The smallest possible time between one observation and the next." ;
              rdfs:isDefinedBy "http://purl.oclc.org/NET/ssnx/ssn" ;
              rdfs:label "Frequency" ;
              rdfs:seeAlso "http://www.w3.org/2005/Incubator/ssn/wiki/SSN_Sensor#MeasuringCapability" .


###  http://purl.oclc.org/NET/ssnx/ssn#Latency
ssn:Latency rdf:type owl:Class ;
            rdfs:subClassOf ssn:MeasurementProperty ;
            rdfs:comment "The time between a request for an observation and the sensor providing a result." ;
            rdfs:isDefinedBy "http://purl.oclc.org/NET/ssnx/ssn" ;
            rdfs:label "Latency" ;
            rdfs:seeAlso "http://www.w3.org/2005/Incubator/ssn/wiki/SSN_Sensor#MeasuringCapability" .


###  http://purl.oclc.org/NET/ssnx/ssn#MaintenanceSchedule
ssn:MaintenanceSchedule rdf:type owl:Class ;
                        rdfs:subClassOf ssn:OperatingProperty ;
                        rdfs:comment "Schedule of maintenance for a system/sensor in the specified conditions." ;
                        rdfs:isDefinedBy "http://purl.oclc.org/NET/ssnx/ssn" ;
                        rdfs:label "Maintenance Schedule" ;
                        rdfs:seeAlso "http://www.w3.org/2005/Incubator/ssn/wiki/SSN_Deploy#OperatingRestriction" .


###  http://purl.oclc.org/NET/ssnx/ssn#MeasurementCapability
ssn:MeasurementCapability rdf:type owl:Class ;
                          rdfs:subClassOf ssn:Property ,
                                          [ rdf:type owl:Restriction ;
                                            owl:onProperty ssn:forProperty ;
                                            owl:allValuesFrom ssn:Property
                                          ] ,
                                          [ rdf:type owl:Restriction ;
                                            owl:onProperty ssn:hasMeasurementProperty ;
                                            owl:allValuesFrom ssn:MeasurementProperty
                                          ] ,
                                          [ rdf:type owl:Restriction ;
                                            owl:onProperty ssn:inCondition ;
                                            owl:allValuesFrom ssn:Condition
                                          ] ;
                          dc:source """Similar idea to MeasurementCapability in MMI Device Ontology
                                  http://marinemetadata.org/community/teams/ontdevices

But the the two express the relationship between constraints and multiple measurement properties differently.

The conditions linked to a MeasurementCapability are skos:exactMatch to 'influence quantity' [VIM 2.52]
http://www.bipm.org/utils/common/documents/jcgm/JCGM_200_2008.pdf""" ;
                          rdfs:comment """Collects together measurement properties (accuracy, range, precision, etc) and the environmental conditions in which those properties hold, representing a specification of a sensor's capability in those conditions.

The conditions specified here are those that affect the measurement properties, while those in OperatingRange represent the sensor's standard operating conditions, including conditions that don't affect the observations.""" ;
                          rdfs:isDefinedBy "http://purl.oclc.org/NET/ssnx/ssn" ;
                          rdfs:label "Measurement Capability" ;
                          rdfs:seeAlso "http://www.w3.org/2005/Incubator/ssn/wiki/SSN_Sensor#MeasuringCapability" .


###  http://purl.oclc.org/NET/ssnx/ssn#MeasurementProperty
ssn:MeasurementProperty rdf:type owl:Class ;
                        rdfs:subClassOf ssn:Property ;
                        rdfs:comment "An identifiable and observable characteristic of a sensor's observations or ability to make observations." ;
                        rdfs:isDefinedBy "http://purl.oclc.org/NET/ssnx/ssn" ;
                        rdfs:label "Measurement  Property" ;
                        rdfs:seeAlso "http://www.w3.org/2005/Incubator/ssn/wiki/SSN_Sensor#MeasuringCapability" .


###  http://purl.oclc.org/NET/ssnx/ssn#MeasurementRange
ssn:MeasurementRange rdf:type owl:Class ;
                     rdfs:subClassOf ssn:MeasurementProperty ;
                     dc:source """skos:exactMatch 'measuring interval/measurement range' [VIM 4.7]
                                  http://www.bipm.org/utils/common/documents/jcgm/JCGM_200_2008.pdf""" ;
                     rdfs:comment "The set of values that the sensor can return as the result of an observation under the defined conditions with the defined measurement properties.  (If no conditions are specified or the conditions do not specify a range for the observed qualities, the measurement range is to be taken as the condition for the observed qualities.)" ;
                     rdfs:isDefinedBy "http://purl.oclc.org/NET/ssnx/ssn" ;
                     rdfs:label "Measurement  Range" ;
                     rdfs:seeAlso "http://www.w3.org/2005/Incubator/ssn/wiki/SSN_Sensor#MeasuringCapability" .


###  http://purl.oclc.org/NET/ssnx/ssn#OperatingPowerRange
ssn:OperatingPowerRange rdf:type owl:Class ;
                        rdfs:subClassOf ssn:OperatingProperty ;
                        rdfs:comment "Power range in which system/sensor is expected to operate." ;
                        rdfs:isDefinedBy "http://purl.oclc.org/NET/ssnx/ssn" ;
                        rdfs:label "Operating Power Range" ;
                        rdfs:seeAlso "http://www.w3.org/2005/Incubator/ssn/wiki/SSN_Energy#EnergyRestriction" .


###  http://purl.oclc.org/NET/ssnx/ssn#OperatingProperty
ssn:OperatingProperty rdf:type owl:Class ;
                      rdfs:subClassOf ssn:Property ;
                      rdfs:comment "An identifiable characteristic of the environmental and other conditions in which the sensor is intended to operate.  May include power ranges, power sources, standard configurations, attachments and the like." ;
                      rdfs:isDefinedBy "http://purl.oclc.org/NET/ssnx/ssn" ;
                      rdfs:label "Operating Property" ;
                      rdfs:seeAlso "http://www.w3.org/2005/Incubator/ssn/wiki/SSN_Deploy#OperatingRestriction" .


###  http://purl.oclc.org/NET/ssnx/ssn#OperatingRange
ssn:OperatingRange rdf:type owl:Class ;
                   rdfs:subClassOf ssn:Property ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty ssn:hasOperatingProperty ;
                                     owl:allValuesFrom ssn:OperatingProperty
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty ssn:inCondition ;
                                     owl:allValuesFrom ssn:Condition
                                   ] ;
                   dc:source """skos:broaderMatch 'reference operating condition' [VIM 4.11]
                                  http://www.bipm.org/utils/common/documents/jcgm/JCGM_200_2008.pdf

The difference is that here we also allow for qualities that aren't VIM influence quantities [VIM 2.52] - for example, a quantity that alters the power requirements, but doesn't affect the measurement properties - conditions specified in MeasurementCapability should be influence quantities.""" ;
                   rdfs:comment "The environmental conditions and characteristics of a system/sensor's normal operating environment.  Can be used to specify for example the standard environmental conditions in which the sensor is expected to operate (a Condition with no OperatingProperty), or how the environmental and other operating properties relate: i.e., that the maintenance schedule or power requirements differ according to the conditions." ;
                   rdfs:isDefinedBy "http://purl.oclc.org/NET/ssnx/ssn" ;
                   rdfs:label "Operating Range" ;
                   rdfs:seeAlso "http://www.w3.org/2005/Incubator/ssn/wiki/SSN_Deploy#OperatingRestriction" .


###  http://purl.oclc.org/NET/ssnx/ssn#Precision
ssn:Precision rdf:type owl:Class ;
              rdfs:subClassOf ssn:MeasurementProperty ;
              dc:source """skos:exactMatch 'measurement precision/precision' [VIM 2.15]
                                   http://www.bipm.org/utils/common/documents/jcgm/JCGM_200_2008.pdf""" ;
              rdfs:comment "The closeness of agreement between replicate observations on an unchanged or similar quality value: i.e., a measure of a sensor's ability to consitently reproduce an observation." ;
              rdfs:isDefinedBy "http://purl.oclc.org/NET/ssnx/ssn" ;
              rdfs:label "Precision" ;
              rdfs:seeAlso "http://www.w3.org/2005/Incubator/ssn/wiki/SSN_Sensor#MeasuringCapability" .


###  http://purl.oclc.org/NET/ssnx/ssn#Property
ssn:Property rdf:type owl:Class ;
             rdfs:subClassOf <http://www.loa-cnr.it/ontologies/DUL.owl#Quality> ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty ssn:isPropertyOf ;
                               owl:someValuesFrom ssn:FeatureOfInterest
                             ] ;
             dc:source """skos:exactMatch 'property' [O&M] 
		                    http://www.opengeospatial.org/standards/om""" ;
             rdfs:comment "A ssn:Property is something that is a DUL:Quality and has a ssn:isPropertyOf property who may be a ssn:FeatureOfInterest"@en ,
                          "An observable Quality of an Event or Object.  That is, not a quality of an abstract entity as is also allowed by DUL's Quality, but rather an aspect of an entity that is intrinsic to and cannot exist without the entity and is observable by a sensor." ;
             rdfs:isDefinedBy "http://purl.oclc.org/NET/ssnx/ssn" ,
                              "http://purl.oclc.org/NET/ssnx/ssn#" ;
             rdfs:label "Property" ,
                        "property (ssn)" ;
             rdfs:seeAlso "http://www.w3.org/2005/Incubator/ssn/wiki/SSN_Skeleton#Skeleton" .


###  http://purl.oclc.org/NET/ssnx/ssn#Resolution
ssn:Resolution rdf:type owl:Class ;
               rdfs:subClassOf ssn:MeasurementProperty ;
               dc:source """skos:exactMatch 'resolution' [VIM 4.14]
                                  http://www.bipm.org/utils/common/documents/jcgm/JCGM_200_2008.pdf""" ;
               rdfs:comment "The smallest difference in the value of a quality being observed that would result in perceptably different values of observation results." ;
               rdfs:isDefinedBy "http://purl.oclc.org/NET/ssnx/ssn" ;
               rdfs:label "Resolution" ;
               rdfs:seeAlso "http://www.w3.org/2005/Incubator/ssn/wiki/SSN_Sensor#MeasuringCapability" .


###  http://purl.oclc.org/NET/ssnx/ssn#ResponseTime
ssn:ResponseTime rdf:type owl:Class ;
                 rdfs:subClassOf ssn:MeasurementProperty ;
                 dc:source """skos:exactMatch 'step response time' [VIM 4.23]
                                  http://www.bipm.org/utils/common/documents/jcgm/JCGM_200_2008.pdf""" ;
                 rdfs:comment "The time between a (step) change inthe value of an observed quality and a sensor (possibly with specified error) 'settling' on an observed value." ;
                 rdfs:isDefinedBy "http://purl.oclc.org/NET/ssnx/ssn" ;
                 rdfs:label "Response time" ;
                 rdfs:seeAlso "http://www.w3.org/2005/Incubator/ssn/wiki/SSN_Sensor#MeasuringCapability" .


###  http://purl.oclc.org/NET/ssnx/ssn#Selectivity
ssn:Selectivity rdf:type owl:Class ;
                rdfs:subClassOf ssn:MeasurementProperty ;
                dc:source """skos:exactMatch 'selectivity' [VIM 4.13]
                                 http://www.bipm.org/utils/common/documents/jcgm/JCGM_200_2008.pdf""" ;
                rdfs:comment "Selectivity is a property of a sensor whereby it provides observed values for one or more qualities such that the values of each quality are independent of other qualities in the phenomenon, body, or substance being investigated." ;
                rdfs:isDefinedBy "http://purl.oclc.org/NET/ssnx/ssn" ;
                rdfs:label "Selectivity" ;
                rdfs:seeAlso "http://www.w3.org/2005/Incubator/ssn/wiki/SSN_Sensor#MeasuringCapability" .


###  http://purl.oclc.org/NET/ssnx/ssn#Sensitivity
ssn:Sensitivity rdf:type owl:Class ;
                rdfs:subClassOf ssn:MeasurementProperty ;
                dc:source """skos:exactMatch 'sensitivity' [VIM 4.12]
                                  http://www.bipm.org/utils/common/documents/jcgm/JCGM_200_2008.pdf""" ;
                rdfs:comment "Sensitivity is the quotient of the change in a result of sensor and the corresponding change in a value of a quality being observed." ;
                rdfs:isDefinedBy "http://purl.oclc.org/NET/ssnx/ssn" ;
                rdfs:label "Sensitivity" ;
                rdfs:seeAlso "http://www.w3.org/2005/Incubator/ssn/wiki/SSN_Sensor#MeasuringCapability" .


###  http://purl.oclc.org/NET/ssnx/ssn#SurvivalProperty
ssn:SurvivalProperty rdf:type owl:Class ;
                     rdfs:subClassOf ssn:Property ;
                     rdfs:comment "An identifiable characteristic that represents the extent of the sensors useful life.  Might include for example total battery life or number of recharges, or, for sensors that are used only a fixed number of times, the number of observations that can be made before the sensing capability is depleted." ;
                     rdfs:isDefinedBy "http://purl.oclc.org/NET/ssnx/ssn" ;
                     rdfs:label "Survival Property" ;
                     rdfs:seeAlso "http://www.w3.org/2005/Incubator/ssn/wiki/SSN_Deploy#OperatingRestriction" .


###  http://purl.oclc.org/NET/ssnx/ssn#SurvivalRange
ssn:SurvivalRange rdf:type owl:Class ;
                  rdfs:subClassOf ssn:Property ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty ssn:hasSurvivalProperty ;
                                    owl:allValuesFrom ssn:SurvivalProperty
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty ssn:inCondition ;
                                    owl:allValuesFrom ssn:Condition
                                  ] ;
                  dc:source """skos:narrowerMatch 'limiting operating condition' [VIM 4.10]
                                  http://www.bipm.org/utils/common/documents/jcgm/JCGM_200_2008.pdf""" ;
                  rdfs:comment "The conditions a sensor can be exposed to without damage: i.e., the sensor continues to operate as defined using MeasurementCapability.  If, however, the SurvivalRange is exceeded, the sensor is 'damaged' and MeasurementCapability specifications may no longer hold." ;
                  rdfs:isDefinedBy "http://purl.oclc.org/NET/ssnx/ssn" ;
                  rdfs:label "Survival Range" ;
                  rdfs:seeAlso "http://www.w3.org/2005/Incubator/ssn/wiki/SSN_Deploy#OperatingRestriction" .


###  http://purl.oclc.org/NET/ssnx/ssn#SystemLifetime
ssn:SystemLifetime rdf:type owl:Class ;
                   rdfs:subClassOf ssn:SurvivalProperty ;
                   rdfs:comment "Total useful life of a sensor/system (expressed as total life since manufacture, time in use, number of operations, etc.)." ;
                   rdfs:isDefinedBy "http://purl.oclc.org/NET/ssnx/ssn" ;
                   rdfs:label "System Lifetime" ;
                   rdfs:seeAlso "http://www.w3.org/2005/Incubator/ssn/wiki/SSN_Deploy#OperatingRestriction" .


###  http://www.loa-cnr.it/ontologies/DUL.owl#Quality
<http://www.loa-cnr.it/ontologies/DUL.owl#Quality> rdf:type owl:Class .


###  http://www.w3.org/2000/01/rdf-schema#Resource
rdfs:Resource rdf:type owl:Class .


###  http://www.w3.org/2003/01/geo/wgs84_pos#SpatialThing
geo:SpatialThing rdf:type owl:Class .


###  http://xmlns.com/foaf/0.1/Image
foaf:Image rdf:type owl:Class .


###  https://w3id.org/def/saref4bldg#BuildingSpace
<https://w3id.org/def/saref4bldg#BuildingSpace> rdf:type owl:Class ;
                                                rdfs:comment "An entity used to define the physical spaces of the building. A building space contains devices or building objects."@en ;
                                                rdfs:isDefinedBy <https://w3id.org/def/saref4bldg#> ;
                                                rdfs:label "Building space"@en .


###  https://w3id.org/saref#Service
saref:Service rdf:type owl:Class ;
              rdfs:comment "A service is a representation of a function to a network that makes the function discoverable, registerable, remotely controllable by other devices in the network. A service can represent one or more functions. A Service is offered by a device that wants (a certain set of) its function(s) to be discoverable, registerable, remotely controllable by other devices in the network. A Service must specify the device that is offering the service, the function(s) to be represented, and the (input and output) parameters necessary to operate the service."@en ;
              rdfs:isDefinedBy saref: ;
              rdfs:label "Service"@en ;
              vs:term_status "unstable" .


